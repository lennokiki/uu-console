{
  "name": "uu-console",
  "version": "0.0.2",
  "description": "An enhanced version of console.log",
  "main": "dist/uu-console.umd.js",
  "module": "dist/uu-console.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test-c": "jest --coverage",
    "build": "rimraf dist/* && rollup -c build/build.js && npm run move",
    "move": "node build/move.js",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [
    "console.log",
    "console",
    "log",
    "uu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lennokiki/uu-console.git"
  },
  "author": "lennokiki",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-buble": "^0.21.3",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^29.0.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "shelljs": "^0.8.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
